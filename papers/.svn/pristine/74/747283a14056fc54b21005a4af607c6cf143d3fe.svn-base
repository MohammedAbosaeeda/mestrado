@article{Sun2010b,
abstract = {Se aborda el concepto de bordersense, como una arquitectura hibrida de WSN, usando la tecnolog\'{\i}a mas avanzada, se propone un framework. Este es capaz de proveer funcionalidad en la detecci\'{o}n de intrusos. Este art\'{\i}culo es pertinente para la investigaci\'{o}n.},
annote = {El cuidado de las fronteras en seguridad nacional ha cobrado real importancia. Temas como la vigilancia de caminos, control de drogas, entrada de ilegales y otros. El monitoreo de fronteras en tiempo real, con alto aseguramiento y minimizando la necesidad de soporte humano. Se introduce en el art\'{\i}culo un nuevo framework de patrones de borde, que involucra tres elementos. Un sensor multimedia que est\'{a} equipado con una video c\'{a}mara, Un sensor escalar equipado con vibraci\'{o}n y un sensor movible que recorre el borde por el aire. Los tres colaboran para proveer la capacidad de detecci\'{o}n. Cada elemento, coopera para finalmente transmitir a un administrador remoto. 
        
El art\'{\i}culo detalla todos los aspectos del framework que opera el borde. Se muestra la t\'{e}cnica y las diferentes tecnolog\'{\i}as, para coordinar los elementos. 
        
        
      },
author = {Sun, Zhi and Wang, Pu and Vuran, Mehmet C. and Al-Rodhaan, Mznah a. and Al-Dhelaan, Abdullah M. and Akyildiz, Ian F.},
