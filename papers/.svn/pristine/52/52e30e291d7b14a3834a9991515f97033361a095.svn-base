void insert(int obj)
{
  // preconditions
  unsigned int size_at_pre = size();
  assert(! contains(obj));

  // Class invariants
  invariants();

  // Method's implementation
  // ...

  // Class invariants
  invariants();

  // postconditions
  assert(size() == size_at_pre + 1);
  assert(contains(obj));
}

void invariants()
{
  assert(size() >= 1);
}
