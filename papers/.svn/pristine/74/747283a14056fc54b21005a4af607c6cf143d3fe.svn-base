@article{WoodA.D.;StankovicJ.A.;VirginiaUniv.Charlottesville2002,
abstract = {Los autores analizan dos protocolos de seguridad para dar mayor protecci\'{o}n a las redes de sensores. El tema es pertinente porque es tal vez el tema mas critico y menos desarrollado},
annote = {Las redes de sensores mantienen la promesa de facilitar el procesamiento de datos en tiempo real y en entornos complejos, ayudando en muchos campos como: a proteger y vigilar a los militares, a los ambientalistas, de seguridad-cr\'{\i}tica a nivel nacional y control de las infraestructuras, los recursos. Sin embargo pueden ser vulnerables y permitir da\~{n}o en el mundo real. Sin los mecanismos de seguridad adecuados, las redes se limitar\'{a}n a entornos limitados y controlados, negando la mayor parte de la promesa que poseen. 
        
La limitada capacidad de los nodos de sensores individuales para impedir el fracaso o el ataque hace que garantizar la disponibilidad de la red sea m\'{a}s dif\'{\i}cil. Para identificar las vulnerabilidades de denegaci\'{o}n de servicio, los autores analizaron dos protocolos eficaces de redes de sensores que no ten\'{\i}a, inicialmente, en cuenta la seguridad. Estos ejemplos demuestran que el examen de seguridad en tiempo de dise\~{n}o es la mejor manera de asegurar el despliegue de redes con \'{e}xito. },
author = {{Wood, A.D.; Stankovic, J.A.; Virginia Univ., Charlottesville}, VA},
journal = {Computer Journal},
keywords = {Bibliografia\_anotada,WSN,lista\_filtrada},
mendeley-tags = {Bibliografia\_anotada,lista\_filtrada},
number = {10},
pages = {54 -- 62},
title = {{Denial of service in sensor networks}},
url = {http://ieeexplore.ieee.org/xpl/freeabs\_all.jsp?arnumber=1039518},
volume = {35},
year = {2002}
}
@article{Abiresearch2010,
abstract = {El art\'{\i}culo refiere al mercado para software de M2M. Revisa los segmentos para unirse a WSN.},
annote = {En este estudio de ABI Research examina el mercado de "software M2M”, las condiciones de proveedores de plataformas de software que permiten m\'{a}s f\'{a}cil, menos costoso y m\'{a}s r\'{a}pido desarrollo de aplicaciones M2M por los desarrolladores de aplicaciones en diversos segmentos de mercado vertical. 
        
Se analizan las Oportunidades y las tendencias de tres segmentos de este mercado: software que permite una conectividad m\'{a}s f\'{a}cil con la infraestructura celular para aplicaciones M2M ("conectado plataformas de dispositivos"), un software que permite a las redes de sensores inal\'{a}mbricos (WSN), ser m\'{a}s f\'{a}cilmente integrado en el sensor basado en M2M aplicaciones ("software del sistema WSN"), y software que permite a los dispositivos remotos que se integren m\'{a}s f\'{a}cilmente en aplicaciones M2M ("plataformas de aplicaciones M2M"). },
author = {Abiresearch},
journal = {AbiResearch},
keywords = {Bibliografia\_anotada,M2M,WSN software,lista\_filtrada},
