pages = {56},
title = {{Software for M2M Applications}},
url = {http://www.abiresearch.com/research/1004508-Software+for+M2M+Applications},
year = {2010}
}
@article{Okhovvat2011,
abstract = {Se aborda el problema de la localizaci\'{o}n en WSN, presentando propiedades de colaboraci\'{o}n en sistemas embebidos con restricciones de latencia y energ\'{\i}a. Se propone dos fases para las tareas de WSN basadas en teor\'{\i}a de colas.},
annote = {Se aborda el estudio de una Red de nodos sensores y nodos actores que est\'{a}n interconectados con enlaces Inal\'{a}mbricos, en donde se asume una arquitectura semiautom\'{a}tica, donde los nodos sensores obtienen informaci\'{o}n del ambiente y la transmiten al elemento sink o a la estaci\'{o}n base. 
        
Las redes WSAN son mostradas como apropiadas para tomar eventos del ambiente con reacciones r\'{a}pidas y en aplicaciones cr\'{\i}ticas, lo cual logra con un algoritmo de localizaci\'{o}n de tareas de forma apropiada. El mapeo de tareas a los actores esta guiado por par\'{a}metros QoS. El algoritmo implementado compara el n\'{u}mero de tareas presupuestadas en el sistema distribuido y reduce los tiempos para completarlas. Usa el algoritmo OLB (Opportunistic load balancing), el cual considera el tiempo de ejecuci\'{o}n de las tareas. Se proponen dos algoritmos uno primero que no toma en cuenta sino el tiempo de ejecuci\'{o}n en cambio el segundo toma en consideraci\'{o}n el balance de carga y la completitud de las tareas. Ambos demuestran cierta efectividad en el prop\'{o}sito. 
      },
author = {Okhovvat, Morteza and Sharifi, Mohsen and Momeni, Hossein},
doi = {10.1016/j.procs.2010.12.082},
file = {::},
issn = {18770509},
journal = {Procedia Computer Science},
keywords = {Bibliografia\_anotada,lista\_filtrada,load balancing,queuing system,task allocation,task completion time,wireless sensor actor network},
mendeley-tags = {Bibliografia\_anotada,lista\_filtrada},
month = jan,
pages = {484--490},
publisher = {Elsevier},
title = {{Task allocation to actors in wireless sensor actor networks: an energy and time aware technique}},
