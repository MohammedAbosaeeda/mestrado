@article{Claycomb2011,
abstract = {Para aplicaciones de seguridad en WSN, debe permitir la identificaci\'{o}n de intrusos. Se definen pol\'{\i}ticas de seguridad, en cada nodo, basadas en un modelo de distribuci\'{o}n de grupo y basado en identidad criptogr\'{a}fica. Es pertinente para la investigaci\'{o}n.},
