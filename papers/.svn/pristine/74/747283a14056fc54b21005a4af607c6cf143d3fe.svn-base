@article{Medagliani2011,
abstract = {El art\'{\i}culo aborda el problema de la aplicaciones de detecci\'{o}n con energ\'{\i}a eficiente en WSN, con la notificaci\'{o}n de transmisi\'{o}n con latencia. Todo depende del numero de nodos presentes. El art\'{\i}culo es pertinente para la investigaci\'{o}n.},
annote = {Este art\'{\i}culo aborda el problema del uso eficiente de la energ\'{\i}a, en donde los nodos sensores son localizados de manera aleatoria, pero en peque\~{n}as \'{a}reas. El dise\~{n}o realizado maximiza el tiempo de vida del sistema, mediante el uso de un switch on y off, de los sensores y de la comunicaci\'{o}n. Esto se logra haciendo que los sensores est\'{e}n seguros que el destino sea eventualmente detectado y la notificaci\'{o}n correspondiente pueda ser notificada en un intervalo de tiempo. 
        
Se extiende el framework basado en un conjunto largo de relevantes medidas. Las contribuciones est\'{a}n en lograr un rendimiento incorporando una capa de sensibilidad y de comunicaci\'{o}n, en t\'{e}rminos de probabilidad de p\'{e}rdidas de detecci\'{o}n del destino, retrasos en la detecci\'{o}n, latencia de notificaci\'{o}n de la transmisi\'{o}n y promedio de consumo de energ\'{\i}a. 
        
Adicionalmente, se presenta un toolbox que permite una configuraci\'{o}n eficiente de los par\'{a}metros para hacer una funci\'{o}n de operaci\'{o}n caracterizada por consumo de energ\'{\i}a y calidad del servicio en t\'{e}rminos de capacidad de detecci\'{o}n y latencia. },
author = {Medagliani, P. and Leguay, J. and Ferrari, G. and Gay, V. and Lopez-Ramos, M.},
doi = {10.1016/j.pmcj.2011.02.004},
file = {::},
issn = {15741192},
journal = {Pervasive and Mobile Computing},
keywords = {Bibliografia\_anotada,lista\_filtrada},
mendeley-tags = {Bibliografia\_anotada,lista\_filtrada},
month = mar,
publisher = {Elsevier B.V.},
title = {{Energy-efficient mobile target detection in Wireless Sensor Networks with random node deployment and partial coverage}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1574119211000290},
year = {2011}
}
