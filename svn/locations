<Location /home/rita/svn/grupo1>
      DAV svn
      SVNPath /home/rita/svn/grupo1
      AuthzSVNAccessFile /home/rita/svn/grupo1/conf/authz
      AuthType Basic
      AuthName "Grupo1"
      AuthUserFile /home/rita/svn/grupo1/conf/svn-authz-file
      Require valid-user
</Location>

