#!/bin/csh
acsun < sbac.txt >sbac.tex

platex sbac.tex
rm -f sbac.tex
#dvips -D 1270 -c 1 -t a4 -F -U -o se.ps se.dvi
if ($1) then
  if ($2) then
    dvips -D 1270 -c 1 -t a4 -F -U -p $1 -n $2 -o sbac.ps sbac.dvi
  else
    dvips -D 1270 -c 1 -t a4 -F -U -p $1 -o sbac.ps sbac.dvi
  endif
else
  dvips -D 1270 -c 1 -t a4 -F -U -o sbac.ps sbac.dvi
endif

