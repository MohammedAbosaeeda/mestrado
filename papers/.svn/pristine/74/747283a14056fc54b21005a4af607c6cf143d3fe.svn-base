@article{Li2011a,
abstract = {Se presenta las caracter\'{\i}sticas inherentes a la agregaci\'{o}n de datos, como una t\'{e}cnica esencial para reducir la comunicaci\'{o}n y prolongar el tiempo de vida de los nodos de la red. Igualmente, se aborda aplicaciones criticas de seguridad. Es pertinente para la investigaci\'{o}n.},
annote = {Una de los principales problemas en las redes de sensores inal\'{a}mbricos, es que las bater\'{\i}as no son recargables, raz\'{o}n por la cual se debe reducir la redundancia de los datos y la cantidad de transmisiones. El principal paradigma utilizado ha sido la agregaci\'{o}n de datos, que elimina la redundancia y reserva energ\'{\i}a. T\'{\i}picamente los sensores son organizados en una jerarqu\'{\i}a de tres niveles enrutados hacia la estaci\'{o}n base. Los nodos act\'{u}an como agregadores, en el proceso de colecci\'{o}n de los datos de sus nodos hijos y regresa los resultados agregados hacia la Base. Sin embargo, las restricciones de energ\'{\i}a son una limitante. 
        
El art\'{\i}culo introduce un esquema de ahorro de energ\'{\i}a y de seguridad alta, para agregaci\'{o}n de datos seguros en WSN. Cada nodo del \'{a}rbol agrega un solo mensaje para la agregaci\'{o}n. Para lo cual, se toman tres factores: Overhead en la comunicaci\'{o}n, agregaci\'{o}n segura y privacidad protegida. Se hace una simulaci\'{o}n para comparar el modelo tradicional llamado SMART, contra el EEHA, que es el modelo propuesto. El resultado final muestra c\'{o}mo se reduce la comunicaci\'{o}n y se mejoran los factores, haciendo que los consumos de las bater\'{\i}as sean m\'{a}s bajos.  
        
      },
author = {Li, Hongjuan and Lin, Kai and Li, Keqiu},
doi = {10.1016/j.comcom.2010.02.026},
file = {::},
issn = {01403664},
journal = {Computer Communications},
keywords = {Bibliografia\_anotada,lista\_filtrada,wireless sensor works},
mendeley-tags = {Bibliografia\_anotada,lista\_filtrada},
month = apr,
number = {4},
pages = {591--597},
publisher = {Elsevier B.V.},
title = {{Energy-efficient and high-accuracy secure data aggregation in wireless sensor networks☆}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0140366410001015},
volume = {34},
year = {2011}
}
