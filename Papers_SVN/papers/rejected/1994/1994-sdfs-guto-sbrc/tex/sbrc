#!/bin/sh
acsun < ../txt/sbrc.txt		>sbrc.tex
acsun < ../txt/intro.txt	>intro.tex
acsun < ../txt/arq.txt		>arq.tex
acsun < ../txt/kernel.txt	>kernel.tex
acsun < ../txt/name.txt		>name.tex
acsun < ../txt/file.txt		>file.tex
acsun < ../txt/buffer.txt	>buffer.tex
acsun < ../txt/end.txt		>end.tex
acsun < ../txt/bib.txt		>bib.tex

platex sbrc.tex 
rm -f *.tex
dvips sbrc.dvi -o
