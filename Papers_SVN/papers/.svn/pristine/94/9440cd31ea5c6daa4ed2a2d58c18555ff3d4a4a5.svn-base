@article{aleph1-smashing-the-stack,
    abstract = {{`smash the stack` [C programming] n. On many C implementations
it is possible to corrupt the execution stack by writing past the end of an
array declared auto in a routine.  Code that does this is said to smash the
stack, and can cause return from the routine to jump to a random address.  This
can produce some of the most insidious data-dependent bugs known to mankind.
Variants include trash the stack, scribble the stack, mangle the stack; the term
mung the stack is not used, as this is never done intentionally. See spam; see
also alias bug, fandango on core, memory leak, precedence lossage, overrun
screw.}},
    author = {One, Aleph},
    chapter = {14},
    citeulike-article-id = {3467779},
    citeulike-linkout-0 =
{http://phrack.com/issues.html?issue=49\&\#38;id=14\#article},
    day = {08},
    journal = {Phrack},
    keywords = {buffer-overflow, c, security, software, vulnerability},
    month = nov,
    number = {49},
    posted-at = {2008-10-31 08:49:51},
    priority = {5},
    title = {{Smashing The Stack For Fun And Profit}},
    url = {http://phrack.com/issues.html?issue=49\&\#38;id=14\#article},
    volume = {7},
    year = {1996}
}

