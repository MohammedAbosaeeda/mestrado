mendeley-tags = {Bibliografia\_anotada,lista\_filtrada},
month = sep,
number = {3},
pages = {468--477},
publisher = {Elsevier B.V.},
title = {{BorderSense: Border patrol through advanced wireless sensor networks}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1570870510001484},
volume = {9},
year = {2010}
}
@inproceedings{Moore2007,
abstract = {"Los \'{u}ltimos diez a\~{n}os han visto la invenci\'{o}n y el despliegue de una gama de sistemas que se organizan de una colecci\'{o}n de nodos con el fin de realizar alguna tarea. Sistemas peer-to-peer surgi\'{o} a finales de 1990, primero como un medio de resistencia a la censura en Internet [1] y luego como un mecanismo de intercambio de archivos. Tecnolog\'{\i}as de comunicaci\'{o}n como WiFi, Bluetooth y soporte Homeplug de corto alcance en red de diferentes dispositivos en entornos dom\'{e}sticos y de oficina, Las Redes de sensores vinieron luego para ser montadas a partir de un gran n\'{u}mero de nodos de bajo coste que pueden estar dispersos en un \'{a}rea de inter\'{e}s y cuyo prop\'{o}sito es realizar alguna tarea como la vigilancia o monitoreo ambiental. Se describen dichas comunicaciones estrategias de forma gen\'{e}rica como ""redes ad-hoc '."},
annote = {El autor propone una administraci\'{o}n basada en mecanismos descentralizados, para proveer eficiencia y robustez en la comunicaci\'{o}n de una red de sensores. El principio est\'{a} basado en la asignaci\'{o}n distribuida de tareas en la cual todos los nodos cooperan para proveer servicios. 
        
Los dispositivos m\'{o}viles conectados a la red, pueden perder su conectividad, pero otros nuevos dispositivos son encadenados. El dise\~{n}o del protocolo, provee herramientas disponibles, para crear firmas digitales o almacenar grandes cantidades de datos. Una clave criptogr\'{a}fica se usa para mantener los servicios. 
        
        
El art\'{\i}culo es relevante ya muestra el  dise\~{n}o de Mecanismos de seguridad en modos de comunicaci\'{o}n en WSN. 
      },
author = {Moore, Tyler and Clulow, Jolyon and Nagaraja, Shishir and Anderson, Ross},
booktitle = {Proceedings of the 4th European conference on Security and privacy in ad-hoc and sensor networks},
