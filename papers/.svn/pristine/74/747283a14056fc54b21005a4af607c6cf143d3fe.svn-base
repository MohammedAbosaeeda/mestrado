@article{Lee2008,
abstract = {Este art\'{\i}culo presenta un algoritmo de detecci\'{o}n de fallas distribuidas para redes de sensores inal\'{a}mbricos. Nodos de sensores defectuosos son identificados en base a las comparaciones entre los nodos vecinos y la difusi\'{o}n de la decisi\'{o}n adoptada en cada nodo. La redundancia de tiempo es utilizada para tolerar los fallos transitorios en la detecci\'{o}n y la comunicaci\'{o}n. Para eliminar el retraso en el esquema de redundancia de tiempo una ventana deslizante se emplea con almacenamiento de resultados en comparaciones anteriores. Los resultados de la simulaci\'{o}n muestran que los nodos de sensores con fallas permanentes se identifican con una alta precisi\'{o}n para una gran gama de tipos de fallas, mientras que la mayor\'{\i}a de los fallos transitorios se toleran con una degradaci\'{o}n insignificante del rendimiento.},
annote = {El esquema de detecci\'{o}n de fallas distribuida para redes de sensores ha sido abordado en muchos estudios, pero con algoritmos complejos. En el documento se presenta un algoritmo simple que detecta nodos con mal funcionamiento pero actuando siempre de manera coordinada. Comparaciones de datos son usadas para diseminar estas fallas en los otros nodos. 
        
Finalmente se propone  un algoritmo de detecci\'{o}n de fallas en una Red de sensores Inal\'{a}mbricos, en donde cada nodo identifica el estado actual de nodos propietarios basados en una comparaci\'{o}n local de los datos censados con otra matriz vieja y de all\'{\i}, se facilita la diseminaci\'{o}n de los resultados del test. Es un algoritmo simple y detecta las fallas presentes en nodos sensores con una alta seguridad y con una muy bajo nivel de falsas alarmas.
        
Es un art\'{\i}culo t\'{e}cnico que presenta un algoritmo \'{o}ptimo de identificaci\'{o}n de fallas en nodos de WSN. Es pertinente y ataca las fallas del sistema 
      },
author = {Lee, M and Choi, Y},
doi = {10.1016/j.comcom.2008.06.014},
file = {::},
issn = {01403664},
journal = {Computer Communications},
keywords = {Bibliografia\_anotada,lista\_filtrada,wireless sensor networks},
mendeley-tags = {Bibliografia\_anotada,lista\_filtrada},
month = sep,
number = {14},
pages = {3469--3475},
title = {{Fault detection of wireless sensor networks}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0140366408003587},
volume = {31},
year = {2008}
}
